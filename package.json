{
  "name": "xiaofan-wechat",
  "version": "1.17.0",
  "description": "Fanfou WeApp",
  "private": true,
  "scripts": {
    "test": "xo && ava",
    "upload": "cli/upload.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fanfoujs/xiaofan-wechat.git"
  },
  "author": "FanfouJS",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fanfoujs/xiaofan-wechat/issues"
  },
  "homepage": "https://github.com/fanfoujs/xiaofan-wechat#readme",
  "devDependencies": {
    "ava": "^2.0.0",
    "chalk": "^2.4.2",
    "deep-keys": "^0.5.0",
    "execa": "^2.0.0",
    "ora": "^3.4.0",
    "xo": "^0.24.0"
  },
  "xo": {
    "globals": [
      "App",
      "wx",
      "Page",
      "getApp",
      "getCurrentPages"
    ],
    "ignores": [
      "src/utils/fanfou-sdk-node/modules/**",
      "src/utils/fanfou-sdk/**"
    ],
    "rules": {
      "ava/no-ignored-test-files": 0,
      "camelcase": 0,
      "import/no-unresolved": 0,
      "max-params": [
        "error",
        6
      ],
      "new-cap": [
        "error",
        {
          "capIsNew": false
        }
      ],
      "prefer-rest-params": 0,
      "space-before-function-paren": [
        "error",
        "always"
      ],
      "unicorn/import-index": 0,
      "unicorn/catch-error-name": [
        "error",
        {
          "name": "err"
        }
      ]
    },
    "semicolon": false,
    "space": 2
  },
  "dependencies": {}
}
